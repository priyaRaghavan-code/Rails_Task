<br/>
<table class="table table-striped table-bordered table-hover">
  <thead class="thead-dark">
    <tr>
      <th>Employee Name</th>
      <th>Employee Id</th>
      <th>Edit</th>
      <th>Delete</th>
    </tr>
  </thead>
  <tbody>
    <% @employees.each do |employee| %>
      <tr>
        <td><%= link_to employee.name,employee_path(employee)%></td>
        <td> <%= employee.id%></td>
        <td><%= link_to 'Edit', edit_employee_path(employee),class: "btn btn-outline-primary btn-sm" %></td>
        <td><%= link_to 'Delete', employee, method: :delete, class: "btn btn-outline-danger btn-sm"%></td>
      </tr>
    <% end %>
  </tbody>
</table>
<%= link_to 'Add Employee', new_employee_path, class: "btn btn-secondary" %>
<%= will_paginate @employees%>